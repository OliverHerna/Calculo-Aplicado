    "from PIL import Image\n",
    "\n",
    "#instancia de una nueva imagen, con el nombre de la variable igual a imagen\n",
    "imagen = Image.open(\"muse.jpg\")\n",
    "\n",
    "#imprimir formato\n",
    "print(imagen.format)\n",
    "\n",
    "#imprimir modo (colores)\n",
    "print(imagen.mode)\n",
    "\n",
    "#imprimir medidas\n",
    "print(imagen.size)\n",
    "\n",
    "#imprimir imagen\n",
    "imagen.show()"
